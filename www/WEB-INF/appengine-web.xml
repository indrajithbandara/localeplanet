<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>localeplanet-hrds</application>

	<runtime>java8</runtime>

	<static-files>
		<include path="/favicon.ico" />
		<include path="/images/**" />
		<include path="/css/**" />
		<include path="/font/**" />
	</static-files>

	<system-properties>
   		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
   		<property name="com.ibm.icu.util.TimeZone.DefaultTimeZoneType" value="JDK" />
   		<!--<property name="com.ibm.icu.util.TimeZone.DefaultTimeZoneType" value="ICU" />-->
	</system-properties>

	<threadsafe>true</threadsafe>

	<version>1</version>

</appengine-web-app>
